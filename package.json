{
  "name": "@boaterbase/bb-react-components",
  "version": "0.0.2",
  "description": "BoaterBase React Components",
  "source": "src/index.js",
  "main": "dist/bb-react-components.js",
  "module": "dist/bb-react-components.esm.js",
  "esmodule": "dist/bb-react-components.modern.js",
  "umd:main": "dist/bb-react-components.umd.js",
  "unpkg": "standalone/bb-react-components.js",
  "jsdelivr": "standalone/bb-react-components.js",
  "files": [
    "dist/",
    "standalone/",
    "src/"
  ],
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "watch-test": "jest --watch --runInBand --detectOpenHandles",
    "dev": "npm run storybook & npm run watch-test",
    "watch": "microbundle watch -o dist/ --jsx React.createElement",
    "build": "NODE_ENV=production npm run build-module && npm run build-standalone && npm run build-css && build-storybook",
    "build-module": "NODE_ENV=production microbundle -o dist/ --jsx React.createElement --no-compress",
    "build-standalone": "NODE_ENV=production microbundle -o standalone/ --external react --jsx React.createElement",
    "build-css": "NODE_ENV=production npx tailwindcss build src/styles.css -o dist/styles.css && csso dist/styles.css --output dist/styles.min.css",
    "build-storybook": "build-storybook -o docs/",
    "explorer": "source-map-explorer standalone/bb-react-components.js",
    "storybook": "start-storybook -p 6006",
    "link-dev-react-hooks": "npm link ../bb-next/node_modules/react",
    "patch": "npm version patch",
    "deploy": "npm publish --access public"
  },
  "keywords": [
    "boaterbase",
    "react",
    "components"
  ],
  "author": "Anthony Corcutt",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-queryparams": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@tailwindcss/ui": "^0.6.0",
    "babel-loader": "^8.1.0",
    "cross-fetch": "^3.0.6",
    "csso-cli": "^3.0.0",
    "jest": "^26.4.2",
    "microbundle": "^0.12.3",
    "postcss-import": "^12.0.1",
    "react-is": "^16.13.1",
    "source-map-explorer": "^2.5.0",
    "tailwindcss": "^1.8.5"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "algoliasearch": "^4.4.0",
    "classnames": "^2.2.6",
    "cloudinary-react": "^1.6.6",
    "firebase": "^0.800.7",
    "formatcoords": "^1.1.3",
    "markdown-to-jsx": "^7.0.0",
    "prop-types": "^15.7.2",
    "react-hook-form": "^6.7.2",
    "react-instantsearch-dom": "^6.7.0",
    "react-map-gl": "^5.2.8",
    "react-outside-click-handler": "^1.3.0",
    "react-spring": "^8.0.27",
    "react-uid": "^2.3.0",
    "slugify": "^1.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BoaterBase/bb-react-components.git"
  }
}
