{
  "name": "@boaterbase/bb-react-components",
  "version": "0.0.1",
  "description": "BoaterBase React Components",
  "source": "src/index.js",
  "main": "dist/bb-react-components.js",
  "umd:main": "dist/bb-react-components.umd.js",
  "module": "dist/bb-react-components.m.js",
  "esmodule": "dist/bb-react-components.modern.js",
  "unpkg": "dist/bb-react-components.js",
  "jsdelivr": "dist/bb-react-components.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run storybook",
    "watch": "microbundle watch -o dist/ --jsx React.createElement",
    "build": "npm run build-bundle && npm run build-css && build-storybook",
    "build-bundle": "microbundle -o dist/ --jsx React.createElement",
    "build-css": "npx tailwindcss build src/styles.css -o dist/styles.css && csso dist/styles.css --output dist/styles.min.css",
    "build-storybook": "build-storybook -o docs/",
    "storybook": "start-storybook -p 6006"
  },
  "keywords": [
    "boaterbase",
    "react",
    "components"
  ],
  "author": "Anthony Corcutt",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@tailwindcss/ui": "^0.5.0",
    "babel-loader": "^8.1.0",
    "csso-cli": "^3.0.0",
    "microbundle": "^0.12.3",
    "postcss-import": "^12.0.1",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "tailwindcss": "^1.7.4"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {
    "algoliasearch": "^4.4.0",
    "classnames": "^2.2.6",
    "cloudinary-react": "^1.6.6",
    "firebase": "^7.19.1",
    "formatcoords": "^1.1.3",
    "markdown-to-jsx": "^6.11.4",
    "prop-types": "^15.7.2",
    "react-instantsearch-dom": "^6.7.0",
    "react-map-gl": "^5.2.7",
    "react-outside-click-handler": "^1.3.0",
    "react-spring": "^8.0.27"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BoaterBase/bb-react-components.git"
  }
}
